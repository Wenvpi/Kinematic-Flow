Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ipOG", "[", 
    RowBox[{"n1_", ",", "n2_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"-", 
      FractionBox["n2", 
       RowBox[{"2", "*", "p"}]]}], 
     RowBox[{"i", "[", 
      RowBox[{"n1", ",", "n2"}], "]"}]}], "-", 
    RowBox[{
     FractionBox[
      RowBox[{"p", "*", "n2"}], "2"], 
     RowBox[{"i", "[", 
      RowBox[{"n1", ",", 
       RowBox[{"n2", "+", "2"}]}], "]"}]}], "+", 
    RowBox[{"I", "*", 
     FractionBox[
      RowBox[{"p", "*", "\[Tau]m"}], "2"], 
     RowBox[{"i", "[", 
      RowBox[{"n1", ",", 
       RowBox[{"n2", "+", "1"}]}], "]"}]}], "+", 
    RowBox[{"I", "*", 
     FractionBox["\[Tau]m", 
      RowBox[{"2", "*", "p"}]], 
     RowBox[{"i", "[", 
      RowBox[{"n1", ",", 
       RowBox[{"n2", "-", "1"}]}], "]"}]}], "+", 
    RowBox[{
     FractionBox["n2", 
      RowBox[{"2", "*", "p"}]], 
     RowBox[{"i", "[", 
      RowBox[{
       RowBox[{"n1", "-", "2"}], ",", 
       RowBox[{"n2", "+", "2"}]}], "]"}]}], "-", 
    RowBox[{"I", "*", 
     FractionBox["\[Tau]m", 
      RowBox[{"2", "*", "p"}]], 
     RowBox[{"i", "[", 
      RowBox[{
       RowBox[{"n1", "-", "2"}], ",", 
       RowBox[{"n2", "+", "1"}]}], "]"}]}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.971019819425345*^9, 3.971019830094634*^9}, {
