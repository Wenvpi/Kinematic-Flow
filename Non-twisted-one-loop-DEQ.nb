       RowBox[{"n1", "-", "2"}], ",", 
       RowBox[{"n2", "+", "2"}]}], "]"}]}], "+", 
    RowBox[{"I", "*", 
     FractionBox["\[Tau]m", "2"], 
     RowBox[{"i", "[", 
      RowBox[{
       RowBox[{"n1", "-", "2"}], ",", 
       RowBox[{"n2", "+", "1"}]}], "]"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"TotalDerivative2", "[", 
    RowBox[{"n1_", ",", "n2_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"d", "-", 
       FractionBox["n1", "2"], "-", "n2"}], ")"}], 
     RowBox[{"i", "[", 
      RowBox[{"n1", ",", "n2"}], "]"}]}], "+", 
    RowBox[{"I", "*", "\[Tau]m", "*", 
     RowBox[{"i", "[", 
      RowBox[{"n1", ",", 
       RowBox[{"n2", "-", "1"}]}], "]"}]}], "+", 
    RowBox[{"I", "*", 
     FractionBox["\[Tau]p", "2"], 
