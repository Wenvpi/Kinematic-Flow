     FractionBox[
      RowBox[{
       SuperscriptBox["p", "2"], "*", "n1"}], "2"], 
     RowBox[{"i", "[", 
      RowBox[{
       RowBox[{"n1", "+", "2"}], ",", "n2"}], "]"}]}], "-", 
    RowBox[{"I", "*", 
     FractionBox[
      RowBox[{
       SuperscriptBox["p", "2"], "*", "\[Tau]p"}], "2"], 
     RowBox[{"i", "[", 
      RowBox[{
       RowBox[{"n1", "+", "1"}], ",", "n2"}], "]"}]}], "-", 
    RowBox[{
     FractionBox["n2", "2"], 
     RowBox[{"i", "[", 
      RowBox[{
       RowBox[{"n1", "+", "2"}], ",", 
       RowBox[{"n2", "-", "2"}]}], "]"}]}], "+", 
    RowBox[{"I", "*", 
     FractionBox["\[Tau]p", "2"], 
     RowBox[{"i", "[", 
      RowBox[{
       RowBox[{"n1", "+", "1"}], ",", 
       RowBox[{"n2", "-", "2"}]}], "]"}]}]}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.971020131788583*^9, 3.971020303360991*^9}, {
