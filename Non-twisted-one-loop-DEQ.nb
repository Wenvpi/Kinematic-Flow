     RowBox[{"i", "[", 
      RowBox[{"n1", ",", 
       RowBox[{"n2", "+", "2"}]}], "]"}]}], "-", 
    RowBox[{"I", "*", 
     FractionBox[
      RowBox[{
       SuperscriptBox["p", "2"], "*", "\[Tau]m"}], "2"], 
     RowBox[{"i", "[", 
      RowBox[{"n1", ",", 
